# Frontend Environment Variables (safe to expose)
# These are used in development for direct API calls
REACT_APP_MAPY_API_KEY=your_api_key_here

# Backend Environment Variables (Vercel Serverless Functions only)
# These must be set in Vercel Dashboard under "Environment Variables"
# DO NOT prefix with REACT_APP_ - these are server-side only
MAPY_API_KEY=your_mapy_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
